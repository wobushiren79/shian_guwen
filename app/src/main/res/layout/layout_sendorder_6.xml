<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:MyViewAttr="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.shian.shianlife.view.writeview.SpinnerViewNormal
        android:id="@+id/write_asl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        MyViewAttr:isImportant="true"
        MyViewAttr:titleName="骨灰处理"></com.shian.shianlife.view.writeview.SpinnerViewNormal>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_1dp"
        android:background="@color/zhy_line_3" />

    <com.shian.shianlife.view.writeview.EditTextViewNormal
        android:id="@+id/write_cemeteryname"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        MyViewAttr:isImportant="true"
        MyViewAttr:titleName="安葬公墓名称"></com.shian.shianlife.view.writeview.EditTextViewNormal>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_1dp"
        android:background="@color/zhy_line_3" />

    <com.shian.shianlife.view.writeview.SpinnerViewNormal
        android:layout_width="match_parent"
        android:id="@+id/write_deadmanidentity"
        android:layout_height="wrap_content"
        MyViewAttr:isImportant="true"
        MyViewAttr:titleName="往生者身份"></com.shian.shianlife.view.writeview.SpinnerViewNormal>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_1dp"
        android:background="@color/zhy_line_3" />

    <com.shian.shianlife.view.writeview.SpinnerViewEdit
        android:layout_width="match_parent"
        android:id="@+id/write_agentmanidentity"
        android:layout_height="wrap_content"
        MyViewAttr:isImportant="true"
        MyViewAttr:titleName="经办人身份"></com.shian.shianlife.view.writeview.SpinnerViewEdit>


</LinearLayout>